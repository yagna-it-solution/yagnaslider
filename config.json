{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "yagna-slider-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/yagnaSlider/YagnaSliderWebPart.js",
          "manifest": "./src/webparts/yagnaSlider/YagnaSliderWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": {
      "path": "https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js",
      "globalName": "jquery"
    },
    "jquery-ui": {
      "path": "https://ajax.googleapis.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.js",
      "globalName": "jquery-ui",
      "globalDependencies": ["jquery"]
    }
  },
  "localizedResources": {
    "YagnaSliderWebPartStrings": "lib/webparts/yagnaSlider/loc/{locale}.js"
  }
}
